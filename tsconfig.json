{
    "compilerOptions": {
        "jsx": "react",
        "lib": [
            "dom",
            "dom.iterable",
            "esnext",
        ],
        "module": "ESNext",
        "moduleResolution": "node",
        "outDir": "dist",
        "target": "ESNext",
        "sourceMap": true,
        "allowJs": true,
        "allowSyntheticDefaultImports": true,
        "esModuleInterop": true,
        "forceConsistentCasingInFileNames": true,
        "noEmit": true,
        "noFallthroughCasesInSwitch": true,
        "resolveJsonModule": true,
        "skipLibCheck": true,
        "strict": true
    }
}